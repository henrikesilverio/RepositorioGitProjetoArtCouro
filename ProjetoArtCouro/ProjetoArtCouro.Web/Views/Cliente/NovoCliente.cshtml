        Portal.Mask();
        Portal.NovoCliente({
            AlertaMensagensSeletor: "#AlertaMensagens",
            UrlDados: "@Url.Action("NovoCliente", "Cliente")",
            $BotaoSalvar: $("#SalvarCliente"),
            $Formulario: $("#formularioCliente")
        });
    </script>
}